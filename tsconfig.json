{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@internal/*": ["./docs/.vuepress/.temp/internal/*"]
    },
    "types": ["webpack-env", "vite/client", "vuepress/client-types"]
  },
  "include": [
      "plugins/**/*",
      "theme/**/*",
      "docs/**/*",
      "scripts/**/*",
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      ".cache",
      ".temp",
      "lib",
      "dist"
  ]
}
